<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.varun.VaSaNi.sample">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/orange"
        android:label="VaSaNi"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ProductActivity" />
        <activity android:name=".checkout.outActivity" />
        <activity
            android:name=".SampleActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".onboarding.WelcomeActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".mail.AActivityOne"
            android:theme="@style/LoginTheme"/>
        <activity
            android:name=".mail.AActivityTwo"
            android:theme="@style/Translucent" />
        <activity
            android:name=".splash.ActivitySplash"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>